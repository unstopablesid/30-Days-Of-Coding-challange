count-odd-even in array
class Solution {
    public int[] countOddEven(int[] arr) {
        // Code here
        int m = 0;
        int n = 0;
        for(int i =0;i<arr.length;i++){
            if(arr[i] % 2 != 0){
            m++;    
            }  
            else{
                n++;
            }
            
        } 
        return new int[]{m, n};
    }
}
